{
	"name": "coffeephp/igbinary",
	"description": "Provides classes for handling Igbinary data.",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "Danny Damsky",
			"email": "dannydamsky99@gmail.com"
		}
	],
	"keywords": [
		"coffeephp",
		"coffee",
		"php",
		"serialized",
		"php-serialized",
		"binary",
		"igbinary",
		"translator",
		"serialize",
		"unserialize",
		"deserialize",
		"serialization",
		"unserialization",
		"deserialization"
	],
	"require": {
		"php": ">=7.4",
		"ext-apcu": "*",
		"ext-igbinary": "*",
		"coffeephp/edi": "1.*"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"coffeephp/component-registry": "1.*",
		"coffeephp/binary": "1.*",
		"phpunit/phpunit": "9.*"
	},
	"autoload": {
		"psr-4": {
			"CoffeePhp\\Igbinary\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CoffeePhp\\Igbinary\\Test\\Integration\\": "test/Integration/",
			"CoffeePhp\\Igbinary\\Test\\Unit\\": "test/Unit/"
		}
	},
	"suggest": {
		"coffeephp/component-registry": "For integration of the component with the CoffeePHP framework.",
		"coffeephp/binary": "For the serializer interfaces to be bound to the PHP-serialized implementation."
	},
	"config": {
		"sort-packages": true,
		"optimize-autoloader": true,
		"apcu-autoloader": true
	},
	"extra": {
		"coffeephp": {
			"registry": [
				{
					"registrar": "CoffeePhp\\Igbinary\\Integration\\IgbinaryComponentRegistrar"
				}
			]
		}
	}
}
